{
  "name": "rain",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "lint": "prettier --write .",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-angular": "^17.0.0",
    "@types/node": "^17.0.42",
    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.js": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        "pascal-case"
      ],
      "type-enum": [
        2,
        "always",
        [
          "chore",
          "build",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  },
  "dependencies": {
    "consola": "^2.15.3",
    "discord.js": "^13.8.0",
    "discord.js-docs": "^0.3.0",
    "wokcommands": "^1.5.3"
  }
}
